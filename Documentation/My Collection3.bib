pages = {190--197},
publisher = {Elsevier B.V.},
title = {{The effects of urban microclimate on outdoor thermal sensation and neutral temperature in hot-summer and cold-winter climate}},
url = {http://dx.doi.org/10.1016/j.enbuild.2016.06.086},
volume = {128},
year = {2016}
}
@article{Liu2019,
abstract = {The multi-zone model is widely used to predict airflow and contaminant transport in large buildings under natural or mechanical ventilation. Selecting appropriate mixing factors and particle deposition velocities for the multi-zone model can compensate for the errors resulting from the model's well-mixing assumption. However, different room types, air change rates and ventilation modes can result in different mixing factors and particle deposition velocities. This study selected three typical room types: Z-type, L-type, and rectangle type (R-type). For each room type, the mixing factors and particle deposition velocities were investigated by the CFD model under different natural ventilation rates (0.5 h−1, 1 h−1, 3 h−1, 6 h−1, 12 h−1 and 20 h−1) and modes (door-inlet, window-inlet). The results showed that the mixing factor of the Z-type room was the highest, and the mixing factors of these rooms were 1.32, 1.28 and 1.13, respectively. In addition, the mixing factors presented a V-shaped distribution as a function of the air exchange rate under the window-inlet mode. The particle deposition velocity increased as the air change rate increased, and also demonstrated that the V-shaped curves as a function of particle size (0.05 $\mu$m, 0.1 $\mu$m, 0.5 $\mu$m, 1 $\mu$m, 2.5 $\mu$m, 5 $\mu$m) varied under different air change rates and room types. The results of mixing factors and particle deposition velocities for different room types, air change rates and ventilation modes can be used to improve the accuracy of the multi-zone model.},
author = {Liu, Xiaoran and Li, Fei and Cai, Hao and Zhou, Bin and Shi, Shanshan and Liu, Jinxiang},
doi = {10.1007/s12273-018-0497-x},
file = {:C$\backslash$:/Users/jdiro/Desktop/git/Documentation/Round2(EnviroSense)/Liu2019{\_}Article{\_}ANumericalInvestigationOnTheMi.pdf:pdf},
isbn = {1227301804},
issn = {19968744},
journal = {Building Simulation},
keywords = {computational fluid dynamics (CFD),mixing factor,multi-zone model,particle deposition velocity},
number = {3},
pages = {465--473},
title = {{A numerical investigation on the mixing factor and particle deposition velocity for enclosed spaces under natural ventilation}},
volume = {12},
year = {2019}
}
