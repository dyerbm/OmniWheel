author = {Abraham, Kirubakaran and Pandian, Shunmugham},
doi = {10.1109/ISMS.2013.76},
file = {:C$\backslash$:/Users/jdiro/Desktop/git/Documentation/Round2(EnviroSense)/06498354.pdf:pdf},
isbn = {9780769549637},
issn = {21660662},
journal = {Proceedings - International Conference on Intelligent Systems, Modelling and Simulation, ISMS},
keywords = {air quality sensors,database,open source hardware,web server},
pages = {659--664},
publisher = {IEEE},
title = {{A low-cost mobile urban environmental monitoring system}},
year = {2013}
}
