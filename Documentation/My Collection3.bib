journal = {Proceedings of 2004 International Conference on Machine Learning and Cybernetics},
keywords = {Dijkstra algorithm,MAKLINK graph,Mobile robot,Mutation operator,Particle swarm optimization,path planning},
number = {August},
pages = {2473--2478},
title = {{Path planning for mobile robot using the particle swarm optimization with mutation operator}},
